      ImportLibrary="$(OutDir)/$(TargetName).lib"
      GenerateMapFile="TRUE"
      MapFileName="$(OutDir)/render_gl2DEBUG_x86.dll.map"
      SubSystem="1"
      />
    <Tool Name="VCMIDLTool"/>
    <Tool Name="VCPreBuildEventTool"/>
    <Tool Name="VCPostBuildEventTool"/>
    <Tool Name="VCPreLinkEventTool"/>
    <Tool Name="VCResourceCompilerTool"/>
    <Tool Name="VCWebServiceProxyGeneratorTool"/>
    <Tool Name="VCXMLDataGeneratorTool"/>
    <Tool Name="VCWebDeploymentTool"/>
    <Tool Name="VCManagedWrapperGeneratorTool"/>
    <Tool Name="VCAuxiliaryManagedWrapperGeneratorTool"/>
  </Configuration>
  <Configuration
    Name="release|Win32"
    OutputDirectory="../../bin/win32"
    IntermediateDirectory="build/Win32/render_gl2/release"
    ConfigurationType="2"
    CharacterSet="2"
    >
    <Tool
      Name="VCCLCompilerTool"
      AdditionalIncludeDirectories="&quot;../../src&quot;;&quot;../../ext&quot;;&quot;../../ext/freeimage/include&quot;;&quot;../../ext/freeglut/include&quot;;&quot;../../ext/glew/include&quot;;&quot;../../ext/anttweakbar/include&quot;;&quot;../../ext/meshimport/include&quot;;"
      PreprocessorDefinitions="WIN32;_WINDOWS;_CRT_SECURE_NO_DEPRECATE;_CRT_NONSTDC_NO_DEPRECATE;UNICODE=1;RENDER_GL2_EXPORTS;NDEBUG;"
      WarningLevel="3"
      Optimization="4"
      AdditionalOptions="/fp:fast /WX /W4 /MD /Zi /O2 "
      ExceptionHandling="1"
      UsePrecompiledHeader="2"
      PrecompiledHeaderThrough="render_gl2_afx.h"
      />
    <Tool Name="VCCustomBuildTool"/>
    <Tool 
      Name="VCLinkerTool" 
      OutputFile="$(OutDir)/render_gl2_x86.dll" 
      ProgramDatabaseFile="$(OutDir)/render_gl2_x86.dll.pdb" 
      AdditionalOptions="/DEBUG /DEBUG "
      AdditionalDependencies="&quot;winmm.lib&quot; &quot;dbghelp.lib&quot; &quot;winmm.lib&quot; &quot;comctl32.lib&quot; &quot;shlwapi.lib&quot; &quot;freeimage.lib&quot; &quot;glew32.lib&quot; &quot;freeglut.lib&quot; &quot;anttweakbar.lib&quot; "
      AdditionalLibraryDirectories="&quot;../../ext/freeglut/lib&quot;;&quot;../../ext/anttweakbar/lib&quot;;&quot;../../ext/freeimage/lib/win32&quot;;&quot;../../ext/glew/lib/win32&quot;;"
      ImportLibrary="$(OutDir)/$(TargetName).lib"
      GenerateMapFile="TRUE"
      MapFileName="$(OutDir)/render_gl2_x86.dll.map"
      SubSystem="1"
      />
    <Tool Name="VCMIDLTool"/>
    <Tool Name="VCPreBuildEventTool"/>
    <Tool Name="VCPostBuildEventTool"/>
    <Tool Name="VCPreLinkEventTool"/>
    <Tool Name="VCResourceCompilerTool"/>
    <Tool Name="VCWebServiceProxyGeneratorTool"/>
    <Tool Name="VCXMLDataGeneratorTool"/>
    <Tool Name="VCWebDeploymentTool"/>
    <Tool Name="VCManagedWrapperGeneratorTool"/>
    <Tool Name="VCAuxiliaryManagedWrapperGeneratorTool"/>
