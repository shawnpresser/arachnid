      WarningLevel="3"
      Optimization="4"
      AdditionalOptions="/fp:fast /W4 /WX /MDd /Zi "
      ExceptionHandling="1"
      UsePrecompiledHeader="2"
      PrecompiledHeaderThrough="render_gl2_afx.h"
      />
    <Tool Name="VCCustomBuildTool"/>
    <Tool 
      Name="VCLinkerTool" 
      OutputFile="$(OutDir)/render_gl2DEBUG_x86.dll" 
      ProgramDatabaseFile="$(OutDir)/render_gl2DEBUG_x86.dll.pdb" 
      AdditionalOptions="/DEBUG /DEBUG "
      AdditionalDependencies="&quot;winmm.lib&quot; &quot;dbghelp.lib&quot; &quot;winmm.lib&quot; &quot;comctl32.lib&quot; &quot;shlwapi.lib&quot; &quot;freeimage.lib&quot; &quot;glew32.lib&quot; &quot;freeglut.lib&quot; &quot;anttweakbar.lib&quot; "
