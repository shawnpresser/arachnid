<?xml version="1.0" encoding="Windows-1252"?>
<VisualStudioProject
ProjectType="Visual C++"
Version="9.00"
Name="engine"
>
<Platforms>
<Platform Name="Win32"/>
</Platforms>
<Configurations>
  <Configuration
    Name="debug|Win32"
    OutputDirectory="../../bin/win32"
    IntermediateDirectory="build/Win32/engine/debug"
    ConfigurationType="2"
    CharacterSet="2"
    >
    <Tool
      Name="VCCLCompilerTool"
      AdditionalIncludeDirectories="&quot;../../src&quot;;&quot;../../ext&quot;;"
      PreprocessorDefinitions="WIN32;_WINDOWS;_CRT_SECURE_NO_DEPRECATE;_CRT_NONSTDC_NO_DEPRECATE;UNICODE=1;ENGINE_EXPORTS;_DEBUG;"
      WarningLevel="3"
      Optimization="4"
      AdditionalOptions="/fp:fast /W4 /WX /MDd /Zi "
      ExceptionHandling="1"
      UsePrecompiledHeader="2"
      PrecompiledHeaderThrough="engine_afx.h"
      />
    <Tool Name="VCCustomBuildTool"/>
    <Tool 
      Name="VCLinkerTool" 
      OutputFile="$(OutDir)/engineDEBUG_x86.dll" 
      ProgramDatabaseFile="$(OutDir)/engineDEBUG_x86.dll.pdb" 
      AdditionalOptions="/DEBUG /DEBUG "
      AdditionalDependencies="&quot;winmm.lib&quot; &quot;dbghelp.lib&quot; &quot;winmm.lib&quot; &quot;comctl32.lib&quot; &quot;shlwapi.lib&quot; &quot;anttweakbar.lib&quot; "
      AdditionalLibraryDirectories="&quot;../../ext/anttweakbar/lib&quot;;"
      ImportLibrary="$(OutDir)/$(TargetName).lib"
      GenerateMapFile="TRUE"
      MapFileName="$(OutDir)/engineDEBUG_x86.dll.map"
      SubSystem="1"
      />
    <Tool Name="VCMIDLTool"/>
    <Tool Name="VCPreBuildEventTool"/>
    <Tool Name="VCPostBuildEventTool"/>
    <Tool Name="VCPreLinkEventTool"/>
    <Tool Name="VCResourceCompilerTool"/>
    <Tool Name="VCWebServiceProxyGeneratorTool"/>
    <Tool Name="VCXMLDataGeneratorTool"/>
    <Tool Name="VCWebDeploymentTool"/>
    <Tool Name="VCManagedWrapperGeneratorTool"/>
    <Tool Name="VCAuxiliaryManagedWrapperGeneratorTool"/>
  </Configuration>
  <Configuration
    Name="release|Win32"
    OutputDirectory="../../bin/win32"
    IntermediateDirectory="build/Win32/engine/release"
    ConfigurationType="2"
    CharacterSet="2"
    >
    <Tool
      Name="VCCLCompilerTool"
      AdditionalIncludeDirectories="&quot;../../src&quot;;&quot;../../ext&quot;;"
      PreprocessorDefinitions="WIN32;_WINDOWS;_CRT_SECURE_NO_DEPRECATE;_CRT_NONSTDC_NO_DEPRECATE;UNICODE=1;ENGINE_EXPORTS;NDEBUG;"
      WarningLevel="3"
      Optimization="4"
      AdditionalOptions="/fp:fast /WX /W4 /MD /Zi /O2 "
      ExceptionHandling="1"
      UsePrecompiledHeader="2"
      PrecompiledHeaderThrough="engine_afx.h"
      />
    <Tool Name="VCCustomBuildTool"/>
    <Tool 
      Name="VCLinkerTool" 
      OutputFile="$(OutDir)/engine_x86.dll" 
      ProgramDatabaseFile="$(OutDir)/engine_x86.dll.pdb" 
      AdditionalOptions="/DEBUG /DEBUG "
      AdditionalDependencies="&quot;winmm.lib&quot; &quot;dbghelp.lib&quot; &quot;winmm.lib&quot; &quot;comctl32.lib&quot; &quot;shlwapi.lib&quot; &quot;anttweakbar.lib&quot; "
      AdditionalLibraryDirectories="&quot;../../ext/anttweakbar/lib&quot;;"
      ImportLibrary="$(OutDir)/$(TargetName).lib"
      GenerateMapFile="TRUE"
      MapFileName="$(OutDir)/engine_x86.dll.map"
      SubSystem="1"
      />
    <Tool Name="VCMIDLTool"/>
    <Tool Name="VCPreBuildEventTool"/>
    <Tool Name="VCPostBuildEventTool"/>
    <Tool Name="VCPreLinkEventTool"/>
    <Tool Name="VCResourceCompilerTool"/>
    <Tool Name="VCWebServiceProxyGeneratorTool"/>
    <Tool Name="VCXMLDataGeneratorTool"/>
    <Tool Name="VCWebDeploymentTool"/>
    <Tool Name="VCManagedWrapperGeneratorTool"/>
    <Tool Name="VCAuxiliaryManagedWrapperGeneratorTool"/>
  </Configuration>
</Configurations>
