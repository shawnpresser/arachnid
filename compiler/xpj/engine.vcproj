<?xml version="1.0" encoding="Windows-1252"?>
<VisualStudioProject
ProjectType="Visual C++"
Version="9.00"
Name="engine"
>
<Platforms>
<Platform Name="Win64"/>
</Platforms>
<Configurations>
  <Configuration
    Name="debug|Win64"
    OutputDirectory="../../bin/win64"
    IntermediateDirectory="build/Win64/engine/debug"
    ConfigurationType="2"
    CharacterSet="2"
    >
    <Tool
      Name="VCCLCompilerTool"
      AdditionalIncludeDirectories="&quot;../../src&quot;;&quot;../../ext&quot;;"
      PreprocessorDefinitions="WIN32;_WINDOWS;_CRT_SECURE_NO_DEPRECATE;UNICODE=1;ENGINE_EXPORTS;_DEBUG;"
      WarningLevel="3"
      Optimization="4"
      AdditionalOptions="/fp:fast /W4 /WX /MDd /Zi "
      ExceptionHandling="1"
      UsePrecompiledHeader="2"
      PrecompiledHeaderThrough="engine_afx.h"
      />
    <Tool Name="VCCustomBuildTool"/>
    <Tool 
      Name="VCLinkerTool" 
      OutputFile="$(OutDir)/engineDEBUG_x64.dll" 
      ProgramDatabaseFile="$(OutDir)/engineDEBUG_x64.dll.pdb" 
      AdditionalOptions="/DEBUG /DEBUG "
      AdditionalDependencies="&quot;winmm.lib&quot; &quot;dbghelp.lib&quot; &quot;winmm.lib&quot; &quot;comctl32.lib&quot; &quot;shlwapi.lib&quot; &quot;freeimage.lib&quot; &quot;freeglut.lib&quot; &quot;anttweakbar64.lib&quot; "
      AdditionalLibraryDirectories="&quot;../../ext/freeimage/lib&quot;;&quot;../../ext/freeglut/lib&quot;;&quot;../../ext/anttweakbar/lib&quot;;"
      ImportLibrary="$(OutDir)/$(TargetName).lib"
      GenerateMapFile="TRUE"
      MapFileName="$(OutDir)/engineDEBUG_x64.dll.map"
      SubSystem="1"
      />
    <Tool Name="VCMIDLTool"/>
    <Tool Name="VCPreBuildEventTool"/>
    <Tool Name="VCPostBuildEventTool"/>
    <Tool Name="VCPreLinkEventTool"/>
    <Tool Name="VCResourceCompilerTool"/>
    <Tool Name="VCWebServiceProxyGeneratorTool"/>
    <Tool Name="VCXMLDataGeneratorTool"/>
    <Tool Name="VCWebDeploymentTool"/>
    <Tool Name="VCManagedWrapperGeneratorTool"/>
    <Tool Name="VCAuxiliaryManagedWrapperGeneratorTool"/>
  </Configuration>
  <Configuration
    Name="release|Win64"
    OutputDirectory="../../bin/win64"
    IntermediateDirectory="build/Win64/engine/release"
    ConfigurationType="2"
    CharacterSet="2"
    >
    <Tool
      Name="VCCLCompilerTool"
      AdditionalIncludeDirectories="&quot;../../src&quot;;&quot;../../ext&quot;;"
      PreprocessorDefinitions="WIN32;_WINDOWS;_CRT_SECURE_NO_DEPRECATE;UNICODE=1;ENGINE_EXPORTS;NDEBUG;"
      WarningLevel="3"
      Optimization="4"
      AdditionalOptions="/fp:fast /WX /W4 /MD /Zi /O2 "
      ExceptionHandling="1"
      UsePrecompiledHeader="2"
      PrecompiledHeaderThrough="engine_afx.h"
      />
    <Tool Name="VCCustomBuildTool"/>
    <Tool 
      Name="VCLinkerTool" 
      OutputFile="$(OutDir)/engine_x64.dll" 
      ProgramDatabaseFile="$(OutDir)/engine_x64.dll.pdb" 
      AdditionalOptions="/DEBUG "
      AdditionalDependencies="&quot;winmm.lib&quot; &quot;dbghelp.lib&quot; &quot;winmm.lib&quot; &quot;comctl32.lib&quot; &quot;shlwapi.lib&quot; &quot;freeimage.lib&quot; &quot;freeglut.lib&quot; &quot;anttweakbar64.lib&quot; "
      AdditionalLibraryDirectories="&quot;../../ext/freeimage/lib&quot;;&quot;../../ext/freeglut/lib&quot;;&quot;../../ext/anttweakbar/lib&quot;;"
      ImportLibrary="$(OutDir)/$(TargetName).lib"
      GenerateMapFile="TRUE"
      MapFileName="$(OutDir)/engine_x64.dll.map"
      SubSystem="1"
      />
    <Tool Name="VCMIDLTool"/>
    <Tool Name="VCPreBuildEventTool"/>
    <Tool Name="VCPostBuildEventTool"/>
    <Tool Name="VCPreLinkEventTool"/>
    <Tool Name="VCResourceCompilerTool"/>
    <Tool Name="VCWebServiceProxyGeneratorTool"/>
    <Tool Name="VCXMLDataGeneratorTool"/>
    <Tool Name="VCWebDeploymentTool"/>
    <Tool Name="VCManagedWrapperGeneratorTool"/>
    <Tool Name="VCAuxiliaryManagedWrapperGeneratorTool"/>
  </Configuration>
</Configurations>
<References></References>
<Files>
  <Filter Name="engine" Filter="">
    <File RelativePath="..\..\src\engine\engine_afx.cpp">
      <FileConfiguration Name="debug|Win64">
        <Tool Name="VCCLCompilerTool" UsePrecompiledHeader="1" />
      </FileConfiguration>
      <FileConfiguration Name="release|Win64">
        <Tool Name="VCCLCompilerTool" UsePrecompiledHeader="1" />
      </FileConfiguration>
    </File>
    <File RelativePath="..\..\src\engine\engine_afx.h">
    </File>
  </Filter>
</Files>
<Globals></Globals>
</VisualStudioProject>
